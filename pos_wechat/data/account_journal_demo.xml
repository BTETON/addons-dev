<odoo noupdate="1">
    <!-- we use type cash, because those journals are added automaticaly to
         Payment Methods on first opening session -->
    <record id="wechat_micropay_journal" model="account.journal">
        <field name="name">WeChat MicroPay</field>
        <field name="wechat">micropay</field>
        <field name="code">WMICRO</field>
        <field name="type">cash</field>
        <field name="journal_user" eval="True"/>
    </record>

    <record id="wechat_native_journal" model="account.journal">
        <field name="name">WeChat Native Payment</field>
        <field name="wechat">native</field>
        <field name="code">WNATIVE</field>
        <field name="type">cash</field>
        <field name="journal_user" eval="True"/>
    </record>
</odoo>
