<?xml version="1.0"?>
<odoo>
    <record id="view_stock_move_line_operation_tree" model="ir.ui.view">
        <field name="name">stock.move.line.operations.tree</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='qty_done']" position="before">
                <field name="product_tracking" invisible="1"/>
                <field name="length" attrs="{'invisible': [('product_tracking', '=', 'none')]}"/>
                <field name="height" attrs="{'invisible': [('product_tracking', '=', 'none')]}"/>
                <field name="width" attrs="{'invisible': [('product_tracking', '=', 'none')]}"/>
            </xpath>
        </field>
    </record>
</odoo>
